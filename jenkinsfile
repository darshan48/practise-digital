pipeline {    
       agent any

     stages {
         stage ('compile stage') {
          steps {
             withMaven(maven : 'maven_3_5_4') {
               sh 'mvn clean compile'
            }
        }
    }
}


     stage ('Testing Stage ') {
        steps {
             withMaven(maven : 'maven_3_5_4') {
              sh 'mvn test'
            }
        }
   }
      stage ('Package Stage ') {
        steps {
             withMaven(maven : 'maven_3_5_4') {
              sh 'mvn package'
             }
       }
   }
}    
